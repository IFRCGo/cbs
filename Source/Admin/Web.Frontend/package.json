{
  "name": "@ifrc-cbs/admin",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.4",
  "description": "CBS Admin frontend React component",
  "scripts": {
    "start": "npm-run-all -l -p node:server parcel:dev",
    "start:local": "npm-run-all -l -p local:server parcel:dev",
    "dev": "npm run parcel:dev",
    "node:server": "cross-env PORT=3001 NODE_ENV=development cbs-node-server",
    "local:server": "cross-env PORT=3001 TESTING_MODE=true NODE_ENV=development cbs-node-server",
    "test": "cross-env HEADLESS_MODE=1 nightwatch",
    "export": "babel --config-file=./.babel/.babelrc --verbose --out-dir=es src && ncp src/assets es/assets",
    "clean": "rimraf es",
    "parcel:dev": "cross-env NODE_ENV=development parcel ./public/index.html",
    "prepare": "npm run clean && npm run export",
    "eslint": "eslint src",
    "preinstall": "cd ../../Navigation/Web.Common && npm install",
    "build": "npm run eslint && npm run prepare"
  },
  "main": "es/exports.js",
  "author": "",
  "license": "MIT",
  "files": [
    "src",
    "es"
  ],
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "@dolittle/commands": "1.0.10",
    "@dolittle/core": "1.0.4",
    "@dolittle/queries": "1.0.12",
    "@dolittle/readmodels": "1.0.0",
    "@ifrc-cbs/common-react-ui": "file:../../Navigation/Web.Common",
    "@ifrc-cbs/reporting": "0.0.4",
    "bootstrap-sass": "^3.4.0",
    "dotenv": "^6.2.0",
    "evergreen-ui": "^4.9.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@ifrc-cbs/nodejs-server": "file:../../Navigation/Web.NodeJS",
    "babel-eslint": "^10.0.1",
    "chromedriver": "^2.45.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.12.3",
    "nightwatch": "^1.0.18",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.11.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "repertoire": "^0.1.6",
    "rimraf": "^2.6.3",
    "sass": "^1.16.0"
  }
}
